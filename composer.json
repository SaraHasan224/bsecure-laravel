{
    "name": "bsecure/bsecure-laravel",
    "description": "Pakistan's first universal checkout solution for ecommerce stores built on woocommerce, magento, shopify and more",
    "keywords": [
        "bsecure",
        "universal-checkout",
        "ecommerce",
        "two-step-checkout",
        "sso",
        "single-sign-on",
        "universal-login",
        "frictionless payment"
    ],
    "homepage": "https://github.com/bSecureCheckout/bsecure-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "bSecure",
            "email": "hello@bsecure.pk",
            "homepage": "https://bsecure.pk",
            "role": "General Support"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.5",
        "guzzlehttp/guzzle": "^7.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "bSecure\\UniversalCheckout\\Tests\\": "tests",
            "bSecure\\UniversalCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bSecure\\UniversalCheckout\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "bSecure\\UniversalCheckout\\CheckoutServiceProvider",
                "bSecure\\UniversalCheckout\\SSOServiceProvider"
            ],
            "aliases": {
                "BsecureCheckout": "bSecure\\UniversalCheckout\\CheckoutFacade",
                "BsecureSSO": "bSecure\\UniversalCheckout\\SSOFacade"
            }
        }
    }
}
